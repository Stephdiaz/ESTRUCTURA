	while (contador =  contador+1 , i<n)		//n+1
	{
		j = 0;	//n
		contador++;
			while (contador =  contador+1 , j<n) //n^2+n
			{
				j++;		//n^2
				contador++;
			}
		i++;		//n
		contador++;
	}
cout << "valor del contador " << contador <<endl;
	getchar();
	getchar();
