// DFD.cpp : main project file.
#include "stdafx.h"
#include <iostream>
using namespace System;
using namespace std;
int main()
{ 
	int i,n,j,k,contador = 0;
	cout << "el valor de n" <<endl;              
	contador++;
	i = 0;
	contador ++;
	cin >> n;	//1
	contador++;
	while (contador=contador+1 , i<n)
	{
		j= 0;
		contador ++;
		while (contador=contador+1 , j<n)
		{
			k = 0;
			contador ++;
			while (contador=contador+1 , k<n)
				{
					k++;
					contador ++;
				}
			j++;
			contador ++;
		}
		i++;
		contador ++;
	}
	cout << "valor del contador " << contador <<endl;
	getchar();
	getchar();
	return 0;

}


//2do ejemplo
