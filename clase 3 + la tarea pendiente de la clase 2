// TAREA.cpp : Defines the entry point for the console application.
//
	
	#include "stdafx.h"
	#include <iostream>
	#include "conio.h"

	using namespace std;

	int main()
	{
		int i,j,n,c, contador;
		contador = 0;
		cout <<"de el valor de n" << endl;		//1
		contador++;
		cin >> n;								//1
		contador++;
		cout << "de el valor de c" << endl;		//1
		contador++;
		cin >> c;								//1
		contador++;
		i = 1;									//1
		contador++;
		while (i<=n)							// LOGc X + 2
		{
			i=i*c;		//c > 1					LOGc X + 1
			contador++;
	
		}
		cout <<" el contador es igual a  "<< contador;
		getch();
		return 0;

	}
	//f(x) = LOG c X+1

	//t(n)= 8 +2 (LOGc X)




